<p id="notice"><%= notice %></p>

<p>
  <h1><%= @last.name %></h1>
</p>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Status</th>
          <th>Street</th>
          <th>City</th>
          <th>Zip</th>
          <th>Country</th>
          <th>Overall</th>
          <th colspan="6"></th>
        </tr>
      </thead>

      <tbody>
        <tr>
          <td><%= @last.name %></td>
          <td><%= @last.status %></td>
          <td><%= @last.street %></td>
          <td><%= @last.city %></td>
          <td><%= @last.zip %></td>
          <td><%= @last.country %></td>
          <td><%= @last.overall %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>

<iframe>
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=#{BeermappingApi.gmaps_key}&q=#{CGI::escape(@last.street @last.city @last.country)}"
</iframe>
